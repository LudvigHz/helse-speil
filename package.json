{
    "name": "helse-speil",
    "version": "1.0.0",
    "author": "Team BÃ¸mlo",
    "description": "Innsyn i saksbehandling av sykepenger",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/helse-speil.git"
    },
    "license": "MIT",
    "scripts": {
        "dev": "turbo run dev --parallel --no-cache",
        "serve": "turbo run serve",
        "mock": "turbo run mock",
        "build": "turbo run build --parallel",
        "test": "turbo run test",
        "clean": "rm -rf node_modules && turbo run clean --parallel",
        "e2e-test": "e2e-tests/run.sh",
        "e2e-test-local": "e2e-tests/run.sh; e2e-tests/stop.sh",
        "generate-graphql": "graphql-codegen --config graphql-codegen.yml",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,json,md}": [
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --rule react-hooks/exhaustive-deps:off"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.5.0",
        "@graphql-codegen/introspection": "^2.1.1",
        "@graphql-codegen/typescript": "^2.4.3",
        "@graphql-codegen/typescript-operations": "^2.2.4",
        "cypress": "^8.6.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^7.0.2",
        "lint-staged": "^11.2.3",
        "prettier": "^2.4.1",
        "turbo": "^1.4.3"
    },
    "engines": {
        "node": ">=14.17.0",
        "npm": ">=6.14.0"
    },
    "snyk": true,
    "workspaces": [
        "packages/*"
    ]
}
