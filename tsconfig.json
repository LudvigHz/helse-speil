{
    "compilerOptions": {
        "moduleResolution": "NodeNext",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noImplicitReturns": true
    },
    "types": ["typePatches"]
}
